  "dependencies": {
    "@11ty/eleventy": "2.0.1",
    "@prismicio/client": "7.1.0",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@swc/core": "1.3.78",
    "autoprefixer": "10.4.14",
    "chokidar-cli": "3.0.0",
    "clean-css": "5.3.2",
    "concurrently": "8.2.0",
    "dotenv": "16.3.1",
