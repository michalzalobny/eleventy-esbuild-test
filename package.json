{
  "name": "eleventy-esbuild-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:pre-eleventy && eleventy",
    "build:pre-eleventy": "npm run scss && npm run esbuild-compile-app",
    "scss": "node scss.js",
    "watch:scss": "nodemon -e scss -x npm run scss",
    "esbuild-compile-app": "node esbuild-compile-app",
    "watch:esbuild-compile-app": "chokidar './src/app/**/*' -c 'npm run esbuild-compile-app'",
    "dev": "npm run build:pre-eleventy && concurrently 'npm run watch:scss' 'npm run watch:esbuild-compile-app' 'eleventy --serve'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://michalzalobny@github.com/michalzalobny/eleventy-esbuild-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michalzalobny/eleventy-esbuild-test/issues"
  },
  "homepage": "https://github.com/michalzalobny/eleventy-esbuild-test#readme",
  "dependencies": {
    "@prismicio/client": "7.1.0",
    "isomorphic-fetch": "3.0.0",
    "fs-extra": "11.1.1",
    "@11ty/eleventy": "2.0.1",
    "@types/node": "20.4.2",
    "nodemon": "3.0.1",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.0",
    "node-sass": "9.0.0",
    "postcss": "8.4.26",
    "autoprefixer": "10.4.14",
    "clean-css": "5.3.2",
    "esbuild": "0.18.13",
    "html-minifier": "4.0.0",
    "dotenv": "16.3.1"
  },
  "devDependencies": {
    "typescript": "5.1.6"
  }
}
